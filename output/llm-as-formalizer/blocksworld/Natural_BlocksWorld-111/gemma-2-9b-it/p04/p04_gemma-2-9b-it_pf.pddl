
(define (problem blocksworld-problem)
  (:domain blocksworld)
  (:objects block1 .. block12 table)
  (:init
    (on block9 table)
    (on block10 block9)
    (on block4 block10)
    (on block2 block4)
    (on block11 block2)
    (on block6 table)
    (on block7 table)
    (on block1 block7)
    (on block12 table)
    (on block5 block12)
    (on block8 block5)
    (on block3 table)
  )
  (:goal
    (and (clear block1)
         (clear block2)
         (clear block3)
         (clear block4)
         (clear block5)
         (clear block6)
         (clear block7)
         (clear block8)
         (clear block9)
         (clear block10)
         (clear block11)
         (clear block12))
  )
)
