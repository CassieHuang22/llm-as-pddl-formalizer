(define (problem blocksworld-problem)
  (:domain blocksworld)
  (:objects b1 b2 b3 b4 b5 b6 b7 b8)
  (:init
    (on b1 b6)
    (clear b1)
    (ontable b6)
    (clear b3)
    (on b2 b3)
    (on b8 b2)
    (on b5 b8)
    (on b7 b5)
    (on b4 b7)
    (ontable b3)
    (arm-empty)
  )
  (:goal
    (and
      (on b8 b3)
      (on b4 b8)
      (on b1 b4)
      (on b2 b1)
      (on b7 b2)
      (on b6 b7)
      (on b5 b6)
      (ontable b3)
    )
  )
)
