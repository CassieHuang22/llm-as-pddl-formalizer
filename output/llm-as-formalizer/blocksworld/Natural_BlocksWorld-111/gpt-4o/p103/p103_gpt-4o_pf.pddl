(define (problem blocksworld-problem)
  (:domain blocksworld)
  (:objects A B C D E F G H - block)
  (:init
    (ontable A)
    (on B A)
    (ontable C)
    (ontable D)
    (on E D)
    (on F E)
    (ontable G)
    (on H G)
    (clear B)
    (clear C)
    (clear F)
    (clear H)
    (arm-empty)
  )
  (:goal (and
    (ontable A)
    (on B A)
    (on C B)
    (ontable D)
    (on E D)
    (on F E)
    (ontable G)
    (on H G))))